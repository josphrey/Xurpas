@model IEnumerable<Xurpas.Models.ParkingSpace>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.ParkingSpaceID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ParkingTypeCode)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.IsAvailable)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingSpaceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingTypeCode)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.IsAvailable)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ParkingSpaceID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ParkingSpaceID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ParkingSpaceID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
